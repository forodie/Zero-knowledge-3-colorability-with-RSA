    "def generateR(colors):\n",
    "    n = len(colors)\n",
    "    listR = []\n",
    "    listRnew = []\n",
    "    for i in range(n):\n",
    "        r = np.random.randint(10,500)\n",
    "        if colors[i] == \"red\":\n",
    "            listR.append(r)\n",
    "            listRnew.append(changeRnew(r,\"red\"))\n",
    "        if colors[i] == \"green\":\n",
    "            listR.append(r)\n",
    "            listRnew.append(changeRnew(r,\"green\"))   \n",
    "        if colors[i] == \"blue\":\n",
    "            listR.append(r)\n",
    "            listRnew.append(changeRnew(r,\"blue\"))\n",
    "    return listR, listRnew"
