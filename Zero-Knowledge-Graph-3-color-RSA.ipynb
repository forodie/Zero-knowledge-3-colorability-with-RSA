   "source": [
    "def changeRnew(r, color):\n",
    "    if color == \"red\":\n",
    "        r = 4*(r//4) + 0\n",
    "    if color == \"green\":\n",
    "        r = 4*(r//4) + 1\n",
    "    if color == \"blue\":\n",
    "        r = 4*(r//4) + 2\n",
    "    return r;"
